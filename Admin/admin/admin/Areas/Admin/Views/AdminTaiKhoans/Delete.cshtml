@model admin.Data.TaiKhoan

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<!-- Spinner Start -->
<div id="spinner" class="show w-100 vh-100 bg-white position-fixed translate-middle top-50 start-50  d-flex align-items-center justify-content-center">
    <div class="spinner-grow text-primary" role="status"></div>
</div>
<!-- Spinner End -->
<!-- Navbar start -->
<div class="container-fluid fixed-top">
    <div class="container topbar bg-primary d-none d-lg-block">
        <div class="d-flex justify-content-between">
            <div class="top-info ps-2">
            </div>

        </div>
    </div>
    <div class="container px-0">
        <nav class="navbar navbar-light bg-white navbar-expand-xl">
            <a href="index.html" class="navbar-brand"><h1 class="text-primary display-6">Admin Caffe Hog Đào</h1></a>
            <div class="d-flex m-3 me-0">
                <a href="#" class="my-auto">
                    <i class="fas fa-user fa-2x"></i>
                </a>
            </div>
        </nav>
    </div>

</div>
<!-- Navbar End -->
<!-- Fruits Shop Start-->
<div class="container-fluid fruite py-5">
    <div class="container py-5">
        <div class="row g-4">
            <div class="col-lg-12">
                <div class="row g-4">
                    <div class="col-lg-3">
                        <div class="row g-4">
                            <div class="col-lg-12">
                                <div class="mb-3">
                                    <ul class="list-unstyled fruite-categorie">
                                        <li>
                                            <div class="d-flex justify-content-between fruite-name">
                                                <a asp-controller="AdminTaiKhoans" asp-action="Index"><i></i>Quản lý tài khoản</a>

                                            </div>
                                        </li>
                                        <li>
                                            <div class="d-flex justify-content-between fruite-name">
                                                <a asp-controller="AdminLoaiTaiKhoans" asp-action="Index"><i></i>Quản lý loại tài khoản</a>

                                            </div>
                                        </li>
                                        <li>
                                            <div class="d-flex justify-content-between fruite-name">
                                                <a href="#"><i></i>Quản lý sản phẩm</a>

                                            </div>
                                        </li>
                                        <li>
                                            <div class="d-flex justify-content-between fruite-name">
                                                <a asp-controller="AdminLoaiSps" asp-action="Index"><i></i>Quản lý loại sản phẩm</a>

                                            </div>
                                        </li>
                                        <li>
                                            <div class="d-flex justify-content-between fruite-name">
                                                <a href="#"><i></i>Quản lý đơn hàng</a>

                                            </div>
                                        </li>
                                        <li>
                                            <div class="d-flex justify-content-between fruite-name">
                                                <a href="#"><i></i>Tổng doanh thu</a>

                                            </div>
                                        </li>
                                        <li>
                                            <div class="d-flex justify-content-between fruite-name">
                                                <a href="#"><i></i>Quản lý khuyến mãi</a>

                                            </div>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-9">
                        <div class="col-lg-9">
                            <!-- Content -->
                            <div class="row g-4 justify-content-center">
                                <div class="col-md-12 col-lg-12 col-xl-12">

                                    <h3>Xóa Tài Khoản </h3>
                                    <div>
                                        <h4>Bạn có chắc muốn xóa không ?</h4>
                                        <hr />
                                        <dl class="row">
                                            <dt class="col-sm-2">
                                                @Html.DisplayNameFor(model => model.TenDn)
                                            </dt>
                                            <dd class="col-sm-10">
                                                @Html.DisplayFor(model => model.TenDn)
                                            </dd>
                                            <dt class="col-sm-2">
                                                @Html.DisplayNameFor(model => model.MatKhau)
                                            </dt>
                                            <dd class="col-sm-10">
                                                @Html.DisplayFor(model => model.MatKhau)
                                            </dd>
                                            <dt class="col-sm-2">
                                                @Html.DisplayNameFor(model => model.HoTen)
                                            </dt>
                                            <dd class="col-sm-10">
                                                @Html.DisplayFor(model => model.HoTen)
                                            </dd>
                                            <dt class="col-sm-2">
                                                @Html.DisplayNameFor(model => model.Email)
                                            </dt>
                                            <dd class="col-sm-10">
                                                @Html.DisplayFor(model => model.Email)
                                            </dd>
                                            <dt class="col-sm-2">
                                                @Html.DisplayNameFor(model => model.Sdt)
                                            </dt>
                                            <dd class="col-sm-10">
                                                @Html.DisplayFor(model => model.Sdt)
                                            </dd>
                                            <dt class="col-sm-2">
                                                @Html.DisplayNameFor(model => model.DiaChi)
                                            </dt>
                                            <dd class="col-sm-10">
                                                @Html.DisplayFor(model => model.DiaChi)
                                            </dd>
                                            <dt class="col-sm-2">
                                                @Html.DisplayNameFor(model => model.IdLoaiTkNavigation.LoaiTk)
                                            </dt>
                                            <dd class="col-sm-10">
                                                @Html.DisplayFor(model => model.IdLoaiTkNavigation.LoaiTk)
                                            </dd>
                                        </dl>

                                        <form asp-action="Delete">
                                            <input type="hidden" asp-for="Id" />
                                            <input type="submit" value="Xóa" class="btn btn-danger" /> |
                                            <a asp-action="Index">Trở lại</a>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Fruits Shop End-->
    <!-- Back to Top -->
    <a href="#" class="btn btn-primary border-3 border-primary rounded-circle back-to-top"><i class="fa fa-arrow-up"></i></a>



